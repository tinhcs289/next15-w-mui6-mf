{
  "name": "@app/blog",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "rm -rf .next && rm -rf .turbo && rm -rf node_modules",
    "build": "cross-env env-cmd -f ./env/.env.localhost next build",
    "build:development": "cross-env env-cmd -f ./env/.env.development next build",
    "build:staging": "cross-env env-cmd -f ./env/.env.staging next build",
    "build:production": "cross-env env-cmd -f ./env/.env.production next build",
    "dev": "cross-env NODE_OPTIONS='--inspect' env-cmd -f ./env/.env.localhost next dev --turbopack",
    "dev:development": "cross-env NODE_OPTIONS='--inspect' env-cmd -f ./env/.env.development next dev --turbopack",
    "dev:staging": "cross-env NODE_OPTIONS='--inspect' env-cmd -f ./env/.env.staging next dev --turbopack",
    "dev:production": "cross-env NODE_OPTIONS='--inspect' env-cmd -f ./env/.env.production next dev --turbopack",
    "publish:windows": "set \"GENERATE_SOURCEMAP=false\" && build:production",
    "publish:linux": "GENERATE_SOURCEMAP=false build:production",
    "start": "env-cmd -f ./env/.env.localhost next start",
    "start:development": "env-cmd -f ./env/.env.development next start",
    "start:staging": "env-cmd -f ./env/.env.staging next start",
    "start:production": "env-cmd -f ./env/.env.production next start",
    "lint": "next lint --max-warnings 0",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@shared/animate-on-scroll": "workspace:*",
    "@shared/auth": "workspace:*",
    "@shared/buttons": "workspace:*",
    "@shared/constants": "workspace:*",
    "@shared/data-table": "workspace:*",
    "@shared/form": "workspace:*",
    "@shared/layouts": "workspace:*",
    "@shared/navigation": "workspace:*",
    "@shared/providers": "workspace:*",
    "@shared/server-actions": "workspace:*",
    "@shared/stack-next-middleware": "workspace:*",
    "@shared/typo": "workspace:*",
    "@shared/utils": "workspace:*",
    "@mui/icons-material": "7.1.0",
    "@mui/material": "7.1.0",
    "@mui/system": "7.1.0",
    "@mui/utils": "7.1.0",
    "@next/third-parties": "15.3.2",
    "@uidotdev/usehooks": "2.4.1",
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "next": "15.3.2",
    "next-intl": "4.1.0",
    "notistack": "3.0.2",
    "numeral": "2.0.6",
    "qs": "6.14.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.56.3"
  },
  "devDependencies": {
    "@shared/config-eslint": "workspace:*",
    "@shared/types": "workspace:*",
    "@types/lodash": "4.17.16",
    "@types/node": "22.15.16",
    "@types/numeral": "2.0.5",
    "@types/qs": "6.9.18",
    "@types/react": "19.1.3",
    "@types/react-dom": "19.1.3",
    "cross-env": "7.0.3",
    "env-cmd": "10.1.0",
    "typescript": "5.8.3"
  }
}